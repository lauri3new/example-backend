{
  "name": "example-backend",
  "scripts": {
    "lint": "eslint --ext .ts .",
    "build": "npx tsc",
    "serve": "",
    "start": "npm run build && node ./dist/index.js",
    "start-prod": "node ./dist/bin/knex-migrate latest && pm2-runtime ./dist/index.js",
    "deploy": "",
    "logs": "",
    "test": "",
    "local:start": "npx tsc -w & npx nodemon -r dotenv/config ./dist/index.js",
    "docker:start": "npx tsc -w & npx nodemon ./dist/main.js",
    "dev:seed": "docker-compose -f ./docker-compose.yml exec app sh -c \"npm run knex-seed-run -- test.js\"",
    "dev:migrate-latest": "docker-compose -f ./docker-compose.yml exec app sh -c \"npm run knex-migrate-latest\"",
    "dev:start": "docker-compose up",
    "dev:down": "docker-compose down",
    "knex-migrate-latest": "node -r dotenv/config ./dist/bin/knex-migrate latest",
    "knex-migrate-rollback": "node -r dotenv/config ./dist/bin/knex-migrate rollback",
    "knex-migrate-version": "node -r dotenv/config ./dist/bin/knex-migrate version",
    "knex-migrate-make": "node -r dotenv/config ./dist/bin/knex-migrate make",
    "knex-seed-run": "node -r dotenv/config ./dist/bin/knex-migrate seed"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@light-arrow/arrow": "^1.1.10",
    "@slack/bolt": "^3.8.1",
    "@slack/web-api": "^6.7.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.12.0",
    "jimp": "^0.16.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.14",
    "morgan": "^1.10.0",
    "objection": "^3.0.0",
    "pg": "^8.7.1",
    "pino": "^7.5.1",
    "pm2": "^5.1.2",
    "stripe": "^8.178.0",
    "tremendous": "^2.1.0",
    "uuid": "^8.3.2",
    "yargs": "^17.3.0",
    "zod": "^3.14.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.3",
    "@types/uuid": "^8.3.1",
    "@types/yargs": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.24.2",
    "firebase-functions-test": "^0.2.3",
    "mocha": "^9.1.2",
    "nodemon": "^2.0.15",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.0.0"
  },
  "private": true
}
